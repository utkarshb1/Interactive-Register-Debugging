<!-- Python functions registering via XML to gnumeric -->
<?xml version="1.0"?>
<plugin id="Gnumeric_MyFuncPlugin">
	<information>
		<name>Other Python functions from</name>
		<description>A few extra python functions demonstrating the API.</description>
	</information>
	<loader type="Gnumeric_PythonLoader:python">
		<attribute name="module_name" value="reg_vf"/>
	</loader>
	<services>
		<service type="function_group" id="example"> 4
			<category>Local Python</category>
			<functions>
				<function name='py_exit'/>
				<function name='py_write'/>
				<function name='py_read'/>
			</functions>
		</service>
	</services>
</plugin>
	